{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/gemini",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ]
}